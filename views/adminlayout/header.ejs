<script>
    document.addEventListener("DOMContentLoaded", function() {
        const navbarAside = document.querySelector('.navbar-aside');
        const body = document.querySelector('body');
        const btnAsideMinimize = document.querySelector('.btn-aside-minimize');

        btnAsideMinimize.addEventListener('click', function() {
            body.classList.toggle('aside-mini');
            body.classList.add('dark'); // Ensure 'dark' class is always added
        });
    });



    document.addEventListener("DOMContentLoaded", function() {
        const body = document.querySelector('body');
        const divScreenOverlay = document.querySelector('.screen-overlay');
        const asideNavbarAside = document.querySelector('.navbar-aside');
        const btnMobile = document.querySelector('.btn-mobile');

        btnMobile.addEventListener('click', function() {
      
            body.classList.toggle('offcanvas-active');
            divScreenOverlay.classList.toggle('show');
            asideNavbarAside.classList.toggle('ps');
            asideNavbarAside.classList.toggle('show');
        });
    });

    $(document).ready(function() {
  // When a .menu-item is clicked
  $('.menu-item').on('click', function(e) {
    e.preventDefault();

    // If this item has a submenu
    if($(this).hasClass('has-submenu')) {
      // Toggle the display of the submenu
      $(this).find('.submenu').toggle();
    }
  });
});

</script>




    